                                                                <li><a href="#">Link</a></li>
                                                                <li><a href="#">Link</a></li>
                                                                <li><a href="#">Link</a></li>
                                                            </ul>
                                                        </div>
                                                    </div>
                                                </li>
                                            </ul>
                                        </div>
                                    </div>
                                </li>
                                <li><a href="#"><span class="glyphicon glyphicon-signal"></span> Link</a></li>*@
                            </ul>
                        </div><!-- /.navbar-collapse -->
                    </nav>
                </div>
            </div>
        </div>
        <div class="col-md-10 content">
            <div class="panel panel-default">
              <div class="container">
                 
                      @RenderBody()
                 
              </div>
                
            </div>
        </div>
        <div class="clearfix"></div>
        <footer class="pull-left footer">
            <p class="col-md-12">
                <hr class="divider">
                © 2018 COBOM. All Rights Reserved | Design by <a href="#" target="_blank">AIM Tech</a>
            </p>
        </footer>
    </div>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)

</body>
</html>